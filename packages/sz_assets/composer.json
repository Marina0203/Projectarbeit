{
	"name": "sunzinet/sz-assets",
	"description": "Provides project specific logic, configurations and templates.",
	"require": {
		"typo3/cms-core": "^12.4"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"typo3/testing-framework": "^8"
	},
	"autoload": {
		"psr-4": {
			"SUNZINET\\SzAssets\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "sz_assets",
			"web-dir": ".Build/Web"
		}
	},
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Christian Efthimoglou",
			"email": "christian.efthimoglou@sunzinet.com",
			"homepage": "https://www.sunzinet.com",
			"role": "Developer"
		}
	]
}
